<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.politeknikharber.DimasShofaGunarso">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DimasShofaGunarso">
        <activity android:name="id.ac.politeknikharber.DimasShofaGunarso.login.LoginActivity"></activity>

        <activity android:name="id.ac.politeknikharber.DimasShofaGunarso.pendidikan.DetailPendidikan"
            android:parentActivityName="id.ac.politeknikharber.DimasShofaGunarso.pendidikan.Pendidikan"/>

        <activity android:name="id.ac.politeknikharber.DimasShofaGunarso.pendidikan.Pendidikan"
            android:parentActivityName="id.ac.politeknikharber.DimasShofaGunarso.MainActivity"/>

        <activity
            android:name="id.ac.politeknikharber.DimasShofaGunarso.calculator.HasilKalkuator"
            android:parentActivityName="id.ac.politeknikharber.DimasShofaGunarso.calculator.Calculator" />

        <activity
            android:name="id.ac.politeknikharber.DimasShofaGunarso.calculator.Calculator"
            android:parentActivityName="id.ac.politeknikharber.DimasShofaGunarso.MainActivity" />

        <activity android:name="id.ac.politeknikharber.DimasShofaGunarso.about.TentangUTS"
            android:parentActivityName="id.ac.politeknikharber.DimasShofaGunarso.MainActivity"/>
        <activity
            android:name="id.ac.politeknikharber.DimasShofaGunarso.SplashScreen"
            android:theme="@style/Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="id.ac.politeknikharber.DimasShofaGunarso.MainActivity" />
    </application>

</manifest>